  pages = {144--156},
  issn = {0031-4773},
  doi = {10.3917/lp.399.0144},
  urldate = {2022-10-05},
  abstract = {Pour tenter de justifier la r{\'e}forme des modalit{\'e}s d'acc{\`e}s {\`a} l'enseignement sup{\'e}rieur, le gouvernement et sa majorit{\'e} parlementaire ont men{\'e} une intense campagne de presse d{\`e}s le printemps 2017. La premi{\`e}re partie vise {\`a} d{\'e}construire le discours gouvernemental ayant servi {\`a} justifier la r{\'e}forme et {\`a} montrer qu'il entretient un rapport pour le moins distant avec la r{\'e}alit{\'e}. {\`a} partir d'une premi{\`e}re analyse des donn{\'e}es de la plateforme Parcoursup, la deuxi{\`e}me partie met en {\'e}vidence les effets s{\'e}gr{\'e}gatifs de la r{\'e}forme, bien {\'e}loign{\'e}s des promesses gouvernementales.},
  langid = {french},
  keywords = {enseignement superieur,Parcoursup,universites}
}

@article{couto_parcoursup_2021,
  title = {{Parcoursup : les pr{\'e}mices d'un accroissement de la stratification sociale et scolaire des formations du sup{\'e}rieur}},
  shorttitle = {{Parcoursup}},
  author = {Couto, Marie-Paule and {Bugeja-Bloch}, Fanny and Frouillou, Le{\"i}la},
  year = {2021},
  month = oct,
  journal = {Agora debats/jeunesses},
  volume = {89},
  number = {3},
  pages = {23--38},
  issn = {1268-5666},
  urldate = {2022-02-18},
  abstract = {Le dispositif d\&\#8217;affectation postbaccalaur\&\#233;at Parcoursup, mis en place en 2018, peut \&\#234;tre analys\&\#233; comme un \&\#233;l\&\#233;ment favorisant la stratification scolaire et sociale des formations sup\&\#233;rieures. Cela tient \&\#224; l\&\#8217;accentuation de la logique du \&\#171;\&\#160;libre choix scolaire\&\#160;\&\#187;, mais aussi \&\#224; la fin de la hi\&\#233;rarchisation des v\&\#339;ux, cruciale dans Admission post-bac (APB), \&\#224; la prise en compte d\&\#8217;\&\#233;l\&\#233;ments extrascolaires dans les candidatures et au renforcement de la canalisation des aspirations scolaires des candidats. En cons\&\#233;quence, les v\&\#339;ux franciliens, entre APB 2017 et Parcoursup 2018, croissent en faveur des fili\&\#232;res STS (sections de technicien sup\&\#233;rieur) et des formations priv\&\#233;es, au d\&\#233;triment des licences publiques universitaires, notamment dans les \&\#233;tablissements non parisiens.},
  langid = {french}
}

@book{desrosieres_pour_2008,
  title = {{Pour une sociologie historique de la quantification : L'Argument statistique I}},
  author = {Desrosi{\`e}res, Alain},
  year = {2008},
  series = {{Sciences sociales}},
  publisher = {Presses des Mines},
  address = {Paris},
  urldate = {2021-10-05},
  abstract = {La quantification est devenue un signe d'objectivit{\'e}, de rigueur et d'impartialit{\'e} mobilis{\'e}e dans des situations tr{\`e}s vari{\'e}es, depuis le d{\'e}bat politique jusqu'{\`a} la d{\'e}monstration scientifique, en passant par les indicateurs d'entreprise ou la mesure de l'opinion publique. Or, la quantification, sous ses diff{\'e}rents formats statistiques, ne se contente pas de fournir un reflet du monde, elle cr{\'e}e une nouvelle fa{\c c}on de le penser, de le repr{\'e}senter, de l'exprimer et d'agir sur lui, {\`a} la fois par la puissance de ses mod{\`e}les et de ses proc{\'e}dures, par leur diffusion et par leurs usages argumentatifs. Ce livre montre comment s'est historiquement construit << l'argument statistique >>, et quels sont aujourd'hui les effets cognitifs et sociaux des dispositifs de quantification. Il ne s'adresse pas uniquement aux sociologues, aux historiens de sciences et aux statisticiens soucieux d'une r{\'e}flexion sur leurs pratiques et leur histoire, mais aussi {\`a} tous les citoyens confront{\'e}s aux appareils statistiques et mesur{\'e}s par eux.},
  isbn = {978-2-35671-090-1},
  langid = {french},
  keywords = {histoire des statistiques sociales,quantification,statistique}
}


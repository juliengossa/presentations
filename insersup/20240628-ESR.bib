@article{strathernImprovingRatingsAudit1997,
  title = {`{{Improving}} Ratings': Audit in the {{British University}} System},
  shorttitle = {`{{Improving}} Ratings'},
  author = {Strathern, Marilyn},
  year = {1997},
  month = jul,
  journal = {European Review},
  volume = {5},
  number = {3},
  pages = {305--321},
  issn = {1474-0575, 1062-7987},
  doi = {10.1002/(SICI)1234-981X(199707)5:3<305::AID-EURO184>3.0.CO;2-4},
  urldate = {2024-06-27},
  abstract = {This paper gives an anthropological comment on what has been called the `audit explosion', the proliferation of procedures for evaluating performance. In higher education the subject of audit (in this sense) is not so much the education of the students as the institutional provision for their education. British universities, as institutions, are increasingly subject to national scrutiny for teaching, research and administrative competence. In the wake of this scrutiny comes a new cultural apparatus of expectations and technologies. While the metaphor of financial auditing points to the important values of accountability, audit does more than monitor---it has a life of its own that jeopardizes the life it audits. The runaway character of assessment practices is analysed in terms of cultural practice. Higher education is intimately bound up with the origins of such practices, and is not just the latter day target of them. {\copyright} 1997 by John Wiley \& Sons, Ltd.},
  langid = {english}
}

@article{vinokurGouvernanceUniversitesPar2017,
  title = {La Gouvernance Des Universit{\'e}s Par La Qualit{\'e}},
  author = {Vinokur, Annie},
  year = {2017},
  month = mar,
  journal = {Recherche et pratiques p{\'e}dagogiques en langues de sp{\'e}cialit{\'e} [En ligne]},
  volume = {Vol. 36},
  pages = {1--17},
  abstract = {Depuis les ann{\'e}es 1990 on observe la multiplication {\`a} l'{\'e}chelle mondiale de dispositifs de mesure et de gestion de la qualit{\'e} {\`a} l'universit{\'e}. Emprunt{\'e}s {\`a} la boite {\`a} outils commerciale et manageriale du Nouveau Management Public, ces dispositifs imposent aux universit{\'e}s une d{\'e}finition de la qualit{\'e} oppos{\'e}e {\`a} la conception traditionnelle qu'ont les universitaires de la qualit{\'e} des savoirs et de leur autonomie professionnelle. Avec le recul du temps on peut maintenant analyser ce ph{\'e}nom{\`e}ne historiquement in{\'e}dit comme participant activement de l'absorption capitaliste d'un secteur public non marchand . Dans cette transition la France est au milieu du gu{\'e}, avec encore le choix d'avancer ou de reculer. Cet article propose donc une interpr{\'e}tation de l'origine et des modalit{\'e}s de l'usage des dispositifs de la qualit{\'e} pour d{\'e}construire l'universit{\'e} traditionnelle. Il montre ensuite un exemple d'aboutissement logique de cette transition. Avec le White Paper de mai 2016 l'Angleterre, qui fut pionni{\`e}re dans le nouveau management de l'universit{\'e}, envisage maintenant d'ouvrir {\`a} tous les fournisseurs, {\`a} but lucratif ou non, un march{\'e} d{\'e}r{\'e}glement{\'e} des universit{\'e}s, avec {\'e}gal acc{\`e}s au financement public par le syst{\`e}me de pr{\^e}ts aux {\'e}tudiants. Ce march{\'e} serait r{\'e}gul{\'e} exclusivement par une agence autonome de la qualit{\'e}, qualit{\'e} mesur{\'e}e par le << value for money >> pour les {\'e}tudiants. Since the 1990s universities have seen a proliferation of methods for quality assessment and management. Borrowed from the New Public Management business model, they impose on universities a definition of quality which is radically opposed to their traditionally held values about quality of knowledge and professional autonomy. This unprecedented movement can now be seen as participating in the capitalist absorption of a non commercial public sector. We endeavour here to analyze its origins and the uses of quality measurements and management techniques in the process. We assess where this transition could lead according to the recent British government White Paper (May 2016) which intends to establish a market open to all providers, for-profit and non-profit, granted equal access to the public funding of students' loans. This market would be regulated by a single non departmental quality agency, concerned only with students' value for money},
  file = {/home/julien/Zotero/storage/5SS9CG55/Vinokur - 2017 - La gouvernance des universités par la qualité.pdf;/home/julien/Zotero/storage/NYEA6JGD/5571.html}
}

